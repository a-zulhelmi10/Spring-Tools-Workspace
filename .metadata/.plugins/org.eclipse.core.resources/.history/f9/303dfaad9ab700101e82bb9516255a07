package testRandom;

public class Randm {
	public String name;
	public String club;
	public int age;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getClub() {
		return club;
	}
	public void setClub(String club) {
		this.club = club;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	Randm(String name,String club,int age){
		this.name=name;
		this.club=club;
		this.age=age;
	}
	public String toString() {
		return name +" " +club +" " +age;
	}
}
